  ami           = "ami-0c55b159cbfafe1f0"  # Example AMI ID, replace with your desired AMI
  instance_type = "t2.micro"  # Instance type

  tags = {
    Name        = "MyEC2Instance"
    Environment = "Development"
    // Add more tags as needed
  }
}
